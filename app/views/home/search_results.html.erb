<div class="row">
	<div class="col-lg-8">
		<table class="table table-striped table-borderless">
			<% if @results.length > 0 %>
				<tr>
					<% @results.first.attributes.each do |attr| %>
						<th><%= attr[0] %></th>
						<% end %>
				</tr>
				<% @results.each do |row| %>
					<tr>
						<% row.attributes.values.each do |value| %>
							<td><%= value %></td>
						<% end %>
					</tr>
				<% end %>
			<% end %>
		</table>
	</div>
</div>