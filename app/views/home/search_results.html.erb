<div class="row">
	<div class="col-lg-8">
		<table class="table table-striped table-borderless">
			<tr>
				<th>シーン</th>
				<th>一般名称/自治体の呼称</th>
				<th>運営/申請窓口</th>
				<th>保障額・内容</th>
			</tr>
			<% if @results.length > 0 %>
				<% @results.each do |row| %>
					<tr>
						<td><%= row[:"2"]%><br /><%= row[:"3"]%></td>
						<td><%= row[:"5"]%></td>
						<td>東京都/居住区の自治体</td>
						<td><%= row[:"7"][0,10]%>...</td>
					</tr>	
				<% end %>
				
			<% end %>
		</table>
	</div>
</div>